{
  "name": "sub-dive",
  "version": "0.1.0",
  "description": "Submarine dive",
  "author": "Renaat",
  "build": {
    "appId": "be.rmeeuws.SubDive",
    "win": {
      "icon": "build/icon.png"
    }
  },
  "main": "public/electron.js",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "google-protobuf": "^3.6.1",
    "grpc-web": "^1.0.7",
    "cross-env": "^7.0.2",
    "electron-is-dev": "^1.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-reflex-grid": "^2.10.0",
    "react-scripts": "^5.0.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "lint": "eslint src/**/*.js* --fix",
    "start-react": "EXTEND_ESLINT=true react-scripts start",
    "build-react": "react-scripts build",
    "test-react": "react-scripts test --env=jsdom",
    "eject-react": "react-scripts eject",
    "build-electron": "electron-builder",
    "build": "npm run build-react && npm run build-electron",
    "start": "concurrently \"cross-env BROWSER=none npm run start-react\" \"wait-on http://localhost:3000 && electron .\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "electron": "^8.1.1",
    "electron-builder": "^22.4.1",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "wait-on": "^4.0.1"
  }
}